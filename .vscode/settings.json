{
    "cSpell.enabled": true,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "editor.insertSpaces": false,
    "files.associations": {
        "*.txt": "c"
    },
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 120,
    },
    "files.autoSave": "afterDelay",
    "cSpell.enableFiletypes": [
        "markdown"
    ]
}